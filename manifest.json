{
  "name": "Ease Items",
  "version": "3.1",
  "icons":{
    "48":"img/CsIcon.png",
    "128":"img/CsIcon.png"
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [ "https://cs.money/ru","http://steamcommunity.com/profiles/*/tradeoffers/","https://steamcommunity.com/tradeoffer/*", "http://tryskins.ru/*","https://www.raffletrades.com/*","https://*.tradeskinsfast.com/"],
      "js": [ "js/jquery.js", "js/background.js" ],
    "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "/injected.js",
    "/injected.css"
  ],
  "browser_action":{
  	"default_icon":"img/CsIcon.png"
  }
}