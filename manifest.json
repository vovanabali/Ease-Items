{
  "name": "Ease Items",
  "version": "2.0",
  "icons":{
    "48":"img/CsIcon.png",
    "128":"img/CsIcon.png"
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [ "https://cs.money/ru","http://steamcommunity.com/profiles/*/tradeoffers/","https://steamcommunity.com/tradeoffer/*"],
      "js": [ "js/jquery.js", "js/background.js" ],
    "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "/injected.js"
  ]
}